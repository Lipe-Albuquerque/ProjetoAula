		super(titulo, opcoes);

	}

	public byte capturarOpcao() {
		byte opcao = super.capturarOpcao();

		switch (opcao) {
		case 1:

			break;

		case 2:

			break;

		default:
			break;
		}
		return opcao;
		
	}

}